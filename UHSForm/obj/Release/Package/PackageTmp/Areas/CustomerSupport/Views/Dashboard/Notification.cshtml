                        </div>
                        <div class="tab-pane fade" id="customersupport">
                            <div class="row mb-4 mt-4" ng-if="SupportList">
                                <div class="col-md-4">
                                    <div class="card text-center p-4" ng-class="{'card-active': activeCard === 'Opening'}" ng-click="setActiveCard('Opening')">
                                        <div class="card-body">
                                            <h5 class="card-title">Open</h5>
                                            <p class="card-text fs-4">{{ openCount }}</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="card text-center p-4"
                                         ng-class="{'card-active': activeCard === 'Pending'}"
                                         ng-click="setActiveCard('Pending')">
                                        <div class="card-body">
                                            <h5 class="card-title">Pending</h5>
                                            <p class="card-text fs-4">{{ pendingCount }}</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="card text-center p-4"
                                         ng-class="{'card-active': activeCard === 'Closed'}"
                                         ng-click="setActiveCard('Closed')">
                                        <div class="card-body">
                                            <h5 class="card-title">Closed</h5>
                                            <p class="card-text fs-4">{{ closeCount }}</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="card mt-3 mb-3" id="datepickerFilter">
                                <!--begin::Card header-->
                                <div class="card-header border-0 pt-6">
                                    <!--begin::Card title-->
                                    <div class="card-title fw-medium">
                                        Filter List
                                    </div>
                                </div>
                                <!--end::Card header-->
                                <div class="row mx-4 my-2" ng-show="SupportList.length!=0">
                                    <!-- Date Range Picker -->
                                    <div class="col-md-5 mb-3">
                                        <label class="form-label required">Range Selection</label>
                                        <input class="form-control"
                                               name="Range"
                                               ng-model="rangewise"
                                               placeholder="Pick date"
                                               id="kt_datepicker_71"
                                               required />
                                        <span ng-if="SearchForm.$submitted && SearchForm.Range.$invalid"
                                              style="color: red">Field is required</span>
                                    </div>
                                    <div class="col-md-2 mt-4 d-flex">
                                        <button type="button" class="btn btn-primary mt-2" ng-click="filterSupportListByDate()">Filter</button>
                                        <button type="button" class="btn btn-secondary mt-2 mx-3" ng-click="resetFilter()">Reset</button>
                                    </div>

                                </div>
                            </div>
                            <!--begin::Card-->
                            <div class="card">

                                <!--begin::Card header-->
                                <div class="card-header border-0 pt-6">
                                    <!--begin::Card title-->
                                    <div class="card-title fw-medium">
                                        Support List
                                    </div>
                                </div>
                                <!--end::Card header-->
                                <!--begin::Card body-->
                                <div class="card-body pt-0">
                                    <div class="table-responsive" id="tbl_supportlist" style="display:none;">
                                        <!--begin::Table-->
                                        <table class="table align-middle table-row-dashed fs-6 gy-5" id="kt_customers_table" datatable="ng" dt-options="vm.dtOptions">
                                            <thead>
                                                <tr class="text-start  fw-bold fs-7 text-uppercase gs-0">
                                                    <th>S.No</th>
                                                    <th>Customer Name</th>
                                                    <th>Ticket Title</th>
                                                    <th style="cursor:pointer;">TicketType</th>
                                                    <th style="cursor:pointer;">
                                                        ServiceType
                                                    </th>
                                                    <th style="cursor:pointer;">ActionFor</th>
                                                    <th>Discription</th>
                                                    <th>Files</th>
                                                    <th>Added On</th>
                                                    <th>Status</th>
                                                    <th ng-if="activeCard != 'Closed'">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody class="fw-semibold text-gray-600">
                                                <tr ng-repeat="support in SupportList">
                                                    <td>{{ $index + 1 }}</td>
                                                    <td ng-bind="support.CustomerName"></td>
                                                    <td ng-bind="support.Title"></td>
                                                    <td ng-bind="support.CustomerSupportServiceTicketType"></td>
                                                    <td ng-bind="support.CustomerSupportServiceType"></td>
                                                    <td ng-bind="support.CustomerSupportActionFor || support.ActionForOther"></td>
                                                    <td ng-bind="support.Remarks"></td>
                                                    <td>
                                                        <a href="" data-bs-toggle="modal" ng-click="FilesList(support.Files)" data-bs-target="#kt_modal_files" ng-bind="support.Files!=null?support.Files.length:'0'"></a>
                                                    </td>
                                                    <td ng-bind="support.CreatedOn| customDate"></td>
                                                    <td>
                                                        <span class="badge" ng-class="{'badge-light-warning': support.CustomerSupportTaskStatus === 'Pending','badge-light-primary': support.CustomerSupportTaskStatus === 'Opening','badge-light-success': support.CustomerSupportTaskStatus === 'Closed'}" ng-bind="support.CustomerSupportTaskStatus"></span>
                                                    </td>
                                                    <td ng-if="activeCard != 'Closed'">
                                                        <a class="btn btn-light btn-active-light-primary btn-flex btn-center btn-sm" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false" ng-disable="support.CustomerSupportTaskStatus === 'Closed'">
                                                            Action <i class="ki-duotone ki-down fs-5 ms-1"></i>
                                                        </a>
                                                        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">

                                                            @*<a class="dropdown-item" href="#" data-bs-toggle="modal"  ng-click="updateAction(support, 1)">Open</a>*@
                                                            <a class="dropdown-item" href="#" data-bs-toggle="modal" ng-show="support.CustomerSupportTaskStatus === 'Opening'" ng-click="updateAction(support, 2)">Pending</a>
                                                            <a class="dropdown-item" href="#" data-bs-toggle="modal" ng-click="updateAction(support, 3)">Close</a>


                                                        </div>
                                                    </td>

                                                </tr>
                                            </tbody>
                                        </table>
                                        <!--end::Table-->
                                    </div>

                                    <div id="tbl_dummysupport">
                                        <table class="table">
                                            <thead>
                                                <tr>
                                                    <th>S.No</th>
                                                    <th>Ticket Title</th>
                                                    <th style="cursor:pointer;">TicketType</th>
                                                    <th style="cursor:pointer;">
                                                        ServiceType
                                                    </th>
                                                    <th style="cursor:pointer;">ActionFor</th>
                                                    <th style="cursor:pointer;">ActionForDetails</th>
                                                    <th>Discription</th>
                                                    <th>Date of Creation</th>
                                                    <th>Status</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td colspan="8" style="color:blue;" id="spanLoader">
                                                        <span><b>Loading...</b></span>
                                                    </td>
                                                    <td colspan="8" style="color:red;display:none;" id="spanEmptyRecords">
                                                        <span>
                                                            No
                                                            records found!
                                                        </span>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <!--end::Card body-->
                            </div>
                            <!--end::Card-->
                        </div>
                        <div class="tab-pane fade" id="specialinstruction">
                            <!--begin::Card header-->
                            <div class="card-header border-0 pt-6">
                                <!--begin::Card title-->
                                <div class="card-title fw-medium">
                                    Special Instruction List
                                </div>
                            </div>
                            <!--end::Card header-->
