                                <!--end::Table-->
                            </div>

                            <div id="tbl_dummyrenewal">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th>S.No</th>
                                            <th class="min-w-125px">Name</th>
                                            <th>Added By</th>
                                            <th>Added On</th>
                                            <th class="min-w-70px">Actions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td colspan="8" style="color:blue;" id="spanrenewalLoader"><span><b>Loading...</b></span></td>
