                                            <th class="min-w-70px">Actions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td colspan="8" style="color:blue;" id="spanspecialLoader"><span><b>Loading...</b></span></td>
                                            <td colspan="8" style="color:red;display:none;" id="spanEmptyspecialRecords"><span>No records found!</span></td>
                                        </tr>
                                    </tbody>
                                </table>
