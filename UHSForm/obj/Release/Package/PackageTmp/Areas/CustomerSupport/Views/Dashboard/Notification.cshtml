                                            </td>
                                            <td ng-bind="not.StaffName|| 'N/A'"></td>
                                            <td ng-bind="not.Review || 'N/A'"></td>
                                            <td>
                                                <a href="" data-bs-toggle="modal" ng-click="FilesList(not.Files)" data-bs-target="#kt_modal_files" ng-bind="not.Files!=null?not.Files.length:'N/A'"></a>
                                            </td>
                                            <td>
                                                <a href="#" class="text-gray-800  mb-1" ng-bind="not.CreatedOn| customDate"></a>
                                            </td>


                                        </tr>


                                    </tbody>
                                </table>
                                <!--end::Table-->
