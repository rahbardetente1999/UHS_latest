
@{
    ViewBag.Title = "Notification";
    Layout = "~/Areas/CustomerSupport/Views/Shared/_LayoutPage.cshtml";
}

<script src="https://d3js.org/d3.v6.min.js"></script>
<style>
    .card-active {
        background-color: transparent;
        color: var(--bs-primary);
        border: 1px solid var(--bs-primary);
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.15);
        transition: background-color 0.3s ease, color 0.3s ease;
        cursor: pointer;
    }

        .card-active .card-title,
        .card-active .card-text {
            cursor: pointer;
        }
</style>

<div class="content d-flex flex-column flex-column-fluid" id="kt_content" ng-controller="StaffRatingController">
    <!--begin::Post-->
    <div class="post d-flex flex-column-fluid" id="kt_post">
        <!--begin::Container-->
        <div id="kt_content_container" class="container-xxl">
            <!--begin::Card-->
            <div class="card">
                <!--begin::Card header-->
                <div class="card-header border-0 pt-6">
                    <!--begin::Navs-->
                    <ul class="nav nav-stretch nav-line-tabs nav-line-tabs-2x border-transparent fs-5 fw-bold">
                        <!--begin::Nav item-->
                        <li class="nav-item mt-2">
                            <a class="nav-link text-active-primary ms-0 me-10 py-5 active" data-bs-toggle="pill" href="#notification">Staff Notification</a>
                        </li>
                        <!--end::Nav item-->
                        <!--begin::Nav item-->
                        <li class="nav-item mt-2">
                            <a class="nav-link text-active-primary ms-0 me-10 py-5" ng-click="RescheduleList()" data-bs-toggle="pill" href="#reschedule">Reschedule List</a>
                        </li>
                        <!--end::Nav item-->
                        <!--begin::Nav item-->
                        <li class="nav-item mt-2">
                            <a class="nav-link text-active-primary ms-0 me-10 py-5" ng-click="GetRenewalList()" data-bs-toggle="pill" href="#renewal">Renewal List</a>
                        </li>
                        <!--end::Nav item-->
                        <!--begin::Nav item-->
                        <li class="nav-item mt-2">
                            <a class="nav-link text-active-primary ms-0 me-10 py-5" ng-click="CustomerSupport()" data-bs-toggle="pill" href="#customersupport">Customer Support</a>
                        </li>
                        <!--end::Nav item-->
                        <!--begin::Nav item-->
                        <li class="nav-item mt-2">
                            <a class="nav-link text-active-primary ms-0 me-10 py-5" ng-click="GetSpecialList()" data-bs-toggle="pill" href="#specialinstruction">Special Instruction</a>
                        </li>
                        <!--end::Nav item-->


                    </ul>
                    <!--begin::Navs-->
                </div>
                <!--end::Card header-->
                <!--begin::Card body-->
                <div class="card-body pt-0">
                    <div class="tab-content">
                        <div class="tab-pane fade show active" id="notification">
                            <div class="table-responsive" id="tbl_packageslist" style="display:none;">
                                <!--begin::Table-->
                                <table class="table align-middle table-row-dashed fs-6 gy-5" id="kt_customers_table" datatable="ng" dt-options="vm.dtOptions">
                                    <thead>
                                        <tr class="text-start  fw-bold fs-7 text-uppercase gs-0">
                                            <th>S.No</th>
                                            <th class="min-w-125px">Customer Name</th>
                                            <th class="min-w-125px">Main Category</th>
                                            <th class="min-w-125px">Sub Category</th>
                                            <th class="min-w-100px">Service Date</th>
                                            <th>Staff Name</th>
                                            <th class="min-w-100px">Comment</th>
                                            <th>Files</th>
                                            <th>Added On</th>

                                        </tr>
                                    </thead>
                                    <tbody class="fw-semibold text-gray-600">
                                        <tr ng-repeat="not in NotificationList">
                                            <td ng-bind="$index+1"></td>
                                            <td>
                                                <a href="" ng-bind="not.CustomerName" data-bs-toggle="modal" data-bs-target="#kt_modal_remilist" ng-click="GetPersonalDtls(not.custID,not.custODID)"></a>
                                            </td>
                                            <td ng-bind="not.MainCategory || 'N/A'"></td>
                                            <td ng-bind="not.SubCategory || 'N/A'"></td>
                                            <td>
