
@{
    Layout = null;
}

<!DOCTYPE html>

<html ng-app="LoginApp">
<head>
    <meta name="google-site-verification" content="V1jFu9dW6FttHSXSom7WyMU6K4EqZc-HPbieT8Is_jI" />
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1, maximum-scale=1">
    <meta name="keywords" content="Urban  Services,  Staffing Qatar, Casual Staff Qatar, Cleaning Services Qatar, Event Staffing Qatar">
    <title>Login - Urban  Services</title>
    <meta name='robots' content='index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1' />

    <!-- This site is optimized with the Yoast SEO plugin v18.5.1 - https://yoast.com/wordpress/plugins/seo/ -->
    <meta name="description" content="Urban  Services is focused on providing best-in-class Hospitality Staffing, Casual Staffing, Event Staffing and Cleaning Services in Qatar." />
    <link rel="canonical" href="https://www.urbanhospitalityservices.com/" />
    <meta property="og:locale" content="en_US" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="Hospitality &amp; Events Staffing | Urban  Services Services" />
    <meta property="og:description" content="Urban  Services Services is focused on providing best-in-class Hospitality Staffing, Casual Staffing, Event Staffing and Cleaning Services in Qatar." />
    <meta property="og:url" content="https://www.urbanhospitalityservices.com/" />
    <meta property="og:site_name" content="Urban Services" />
    <meta property="article:modified_time" content="2021-09-28T04:45:12+00:00" />
    <meta name="twitter:card" content="summary_large_image" />
   
    <link rel="shortcut icon" href="~/Images/logo.png" />
    <!--begin::Fonts(mandatory for all pages)-->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Inter:300,400,500,600,700" />
    <!--end::Fonts-->
    <!--begin::Global Stylesheets Bundle(mandatory for all pages)-->
    <link href="~/Styles/Content/plugins.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/Styles/Content/style.bundle.css" rel="stylesheet" type="text/css" />
</head>
<body id="kt_body" class="auth-bg bgi-size-cover bgi-attachment-fixed bgi-position-center bgi-no-repeat" ng-controller="LoginController">
    <!--begin::Theme mode setup on page load-->
    <script>var defaultThemeMode = "light"; var themeMode; if (document.documentElement) { if (document.documentElement.hasAttribute("data-bs-theme-mode")) { themeMode = document.documentElement.getAttribute("data-bs-theme-mode"); } else { if (localStorage.getItem("data-bs-theme") !== null) { themeMode = localStorage.getItem("data-bs-theme"); } else { themeMode = defaultThemeMode; } } if (themeMode === "system") { themeMode = window.matchMedia("(prefers-color-scheme: dark)").matches ? "dark" : "light"; } document.documentElement.setAttribute("data-bs-theme", themeMode); }</script>
    <!--end::Theme mode setup on page load-->
    <!--begin::Main-->
    <!--begin::Root-->
    <div class="d-flex flex-column flex-root">
        <!--begin::Page bg image-->
        <style>
            body {
                background-image: url('../../Images/Admin/bg6.jpg');
            }

            [data-bs-theme="dark"] body {
                background-image: url('../../Images/Admin/bg6-dark.jpg');
            }
        </style>
        <!--end::Page bg image-->
        <!--begin::Authentication - Sign-in -->
        <div class="d-flex flex-column flex-column-fluid flex-lg-row">
            <!--begin::Aside-->
            <div class="d-flex flex-center w-lg-50 pt-15 pt-lg-0 px-10">
                <!--begin::Aside-->
                <div class="d-flex flex-center flex-lg-start flex-column">
                  
                    <!--begin::Logo-->
                    <a href="/Home/Index" class="mb-7">
                        <img alt="Logo" src="~/Images/logo.png" />
                    </a>
                    <!--end::Logo-->
                    <!--begin::Title-->
                    <h2 class="m-0" style="font-weight: 600 !important;">Excellence in Cleaning &amp;  Staffing</h2>
                    <!--end::Title-->
                </div>
                <!--begin::Aside-->
            </div>
            <!--begin::Aside-->
            <!--begin::Body-->
            <div class="d-flex flex-column-fluid flex-lg-row-auto justify-content-center justify-content-lg-end p-12 p-lg-20">
                <!--begin::Card-->
                <div class="bg-body d-flex flex-column align-items-stretch flex-center rounded-4 w-md-600px p-20">
                    <!--begin::Wrapper-->
                    <div class="d-flex flex-center flex-column flex-column-fluid px-lg-10 pb-15 pb-lg-20">
                        <!--begin::Form-->
                        <form class="form w-100" name="LoginForm" novalidate>
                            <!--begin::Heading-->
                            <div class="text-center mb-11">
                                <!--begin::Title-->
                                <h1 class="text-gray-900 fw-bolder mb-3">Sign In</h1>
                                <!--end::Title-->
                                <!--begin::Subtitle-->
                                <div class="text-gray-500 fw-semibold fs-6">Hello! Welcome to Urban  Services</div>
                                <!--end::Subtitle=-->
                            </div>
                            <!--begin::Heading-->
                            <!--begin::Input group=-->
                            <div class="fv-row mb-8">
                                <label class="fs-6 fw-semibold form-label mt-3">
                                    <span class="required">Username/MobileNo</span>
                                </label>
                                <!--begin::Email-->
                                <input class="form-control bg-transparent" id="emailID" autocomplete="off" type="text" name="email" ng-model="txtLoginEmail" required />
                                <!--end::Email-->
                                <span ng-if="LoginForm.$submitted && LoginForm.email.$invalid" style="color:red" ng-bind="msgReqLoginEmail"></span>

                            </div>
                            <!--end::Input group=-->
                            <div class="fv-row mb-10">
                                <label class="fs-6 fw-semibold form-label mt-3">
                                    <span class="required">Password</span>
                                </label>
                                <!--begin::Password-->
                                <input ng-model="txtPassword" type="password" id="Passwordm" name="Password" required autocomplete="off" class="form-control bg-transparent" />
                                <!--end::Password-->
                                <span ng-if="LoginForm.$submitted && LoginForm.Password.$invalid" style="color:red" ng-bind="msgReqLoginPwd"></span>

                            </div>
                            <!--end::Input group=-->
                            <!--begin::Wrapper-->
                            <div class="d-flex flex-stack flex-wrap gap-3 fs-base fw-semibold mb-8">
                                <div></div>
                                <!--begin::Link-->
                                <a href="/Account/ForgotPassword" class="link-primary">Forgot Password ?</a>
                                <!--end::Link-->
                            </div>
                            <!--end::Wrapper-->
                            <!--begin::Submit button-->
                            <div class="d-grid mb-10">
                                <button type="submit" ng-click="Login(LoginForm.$valid)" class="btn btn-primary">
                                    <!--begin::Indicator label-->
                                    <span class="indicator-label" id="submit">Sign In</span>
                                    <!--end::Indicator label-->
                                    <!--begin::Indicator progress-->
                                    <span class="indicator-progress" id="btnLoginLoader">
                                        Please wait...
                                        <span class="spinner-border spinner-border-sm align-middle ms-2"></span>
                                    </span>
                                    <!--end::Indicator progress-->
                                </button>
                            </div>
                            <!--end::Submit button-->
                            <div class="d-flex flex-stack flex-wrap gap-3 fs-base fw-semibold mb-8">
                                <!--begin::Sign up-->
                                <a href="/Home/Index" class="link-primary">
                                    New customer?

                                </a>
                                <!--end::Sign up-->
                                <!--begin::Link-->
                                <a href="https://urbanservices-qa.com/" target="_blank" class="link-primary"> Go to Website?</a>
                                <!--end::Link-->
                            </div>
                            

                        </form>
                        <!--end::Form-->
                    </div>
                    <!--end::Wrapper-->

                </div>
                <!--end::Card-->
            </div>
            <!--end::Body-->
        </div>
        <!--end::Authentication - Sign-in-->
    </div>
    <!--end::Root-->
    <script src="~/Scripts/Content/plugins.bundle.js"></script>
    <script src="~/Scripts/Content/scripts.bundle.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/App/login.js?v=2"></script>
    <script src="~/Scripts/App/Auth.js"></script>
    <script src="~/Scripts/Content/toastr.js"></script>
</body>
</html>
