    <script>
        // Get the current URL path
        var currentPath = window.location.pathname;

        // Find the menu items and accordion triggers
        var menuLinks = document.querySelectorAll('.menu-link');
        var accordionTriggers = document.querySelectorAll('[data-kt-menu-trigger="click"]');

        // Loop through each menu item
        menuLinks.forEach(function (menuItem) {
            // Get the href attribute of the menu item
            var href = menuItem.getAttribute('href');

            // Check if the current path matches the href attribute
            if (currentPath === href) {
                // Add the 'active' class to the menu item
                menuItem.classList.add('active');

                // If it's a submenu item, add 'active' class to its parent menu as well
                var parentMenu = menuItem.closest('.menu-item');
                if (parentMenu) {
                    parentMenu.classList.add('active');

                    // Find the closest accordion trigger and simulate a click event
                    var accordionTrigger = parentMenu.closest('[data-kt-menu-trigger="click"]');
                    if (accordionTrigger) {
                        // Add Bootstrap classes to open the accordion
                        accordionTrigger.classList.add('here');
                        accordionTrigger.classList.add('show');

                        // If using Bootstrap 5, you may need to use the Bootstrap JavaScript API to open the accordion:
                        // var accordion = new bootstrap.Collapse(accordionTrigger);
                        // accordion.show();
                    }
                }
            }
        });
