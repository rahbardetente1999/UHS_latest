                                        <!--begin::Scroll-->
                                        <!--begin::Input group-->
                                        <!--<div class="fv-row mb-4">-->
                                        <!--begin::Label-->
                                        <!--<label class="required fs-6 fw-semibold mb-2">Username</label>-->
                                        <!--end::Label-->
                                        <!--begin::Input-->
                                        <!--<input type="text" class="form-control" ng-model="txtusername" placeholder="" name="Username" required />-->
                                        <!--end::Input-->
                                        <!--<span ng-if="myAddUserForm.$submitted && myAddUserForm.Username.$invalid" style="color:red" ng-bind="msgVUsername"></span>
                        </div>-->
                                        <!--end::Input group-->
                                        <!--begin::Input group-->
                                        <div class="fv-row mb-4">
                                            <!--begin::Label-->
                                            <label class="required fs-6 fw-semibold mb-2">Name</label>
                                            <!--end::Label-->
                                            <!--begin::Input-->
                                            <input type="text" class="form-control" ng-model="txtname" placeholder="" name="Name" required />
                                            <!--end::Input-->
                                            <span ng-if="myAddUserForm.$submitted && myAddUserForm.Name.$invalid" style="color:red" ng-bind="msgVName"></span>
                                        </div>
                                        <!--end::Input group-->
                                        <!--begin::Input group-->
                                        <div class="fv-row mb-4">
                                            <!--begin::Label-->
                                            <label class="fs-6 fw-semibold mb-2">Email</label>
                                            <!--end::Label-->
                                            <!--begin::Input-->
                                            <input type="email" ng-model="txtEmail" class="form-control" placeholder="" name="Email" />
                                            <span style="color:red" ng-bind="msgVEmail"></span>
                                            <!--end::Input-->
                                        </div>
                                        <!--end::Input group-->
                                        <!--begin::Input group-->
                                        <div class="fv-row mb-4">
                                            <!--begin::Label-->
                                            <label class="required fs-6 fw-semibold mb-2">Mobile No</label>
                                            <!--end::Label-->
                                            <input type="text" id="txtMobileNo" ng-minlength="8" ng-maxlength="8" minlength="8" maxlength="8" name="MobileNo" onkeyup="if (/\D/g.test(this.value)) this.value = this.value.replace(/\D/g,'')" size="80" class="form-control" required ng-model="txtMobileNo" placeholder="Eg: 90000000">
                                            <!-- Error Message for Minlength -->
                                            <span ng-if="myAddUserForm.$submitted && myAddUserForm.MobileNo.$error.minlength"
                                                  style="color:red" ng-bind="msgMinLength || 'Mobile no must be at least 8 digits long.'">

                                            </span><br />
                                            <span ng-if="myAddUserForm.$submitted && myAddUserForm.MobileNo.$invalid" style="color:red" ng-bind="msgVMobileNo"></span>

                                        </div>
                                        <!--end::Input group-->
