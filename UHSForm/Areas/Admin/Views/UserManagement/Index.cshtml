                    <div class="modal fade" id="updatepassword" tabindex="-1" aria-hidden="true">
                        <!--begin::Modal dialog-->
                        <div class="modal-dialog modal-dialog-centered mw-650px">
                            <!--begin::Modal content-->
                            <div class="modal-content">
                                <!--begin::Form-->
                                <form class="form" name="passwordForm" novalidate>
                                    <!--begin::Modal header-->
                                    <div class="modal-header" id="kt_modal_add_customer_header">
                                        <!--begin::Modal title-->
                                        <h2 class="fw-bold">Setup New Password</h2>
                                        <!--end::Modal title-->
                                        <!--begin::Close-->
                                        <div data-bs-dismiss="modal" class="btn btn-icon btn-sm btn-active-icon-primary">
                                            <i class="ki-outline ki-cross fs-1"></i>
                                        </div>
                                        <!--end::Close-->
                                    </div>
                                    <!--end::Modal header-->
                                    <!--begin::Modal body-->
                                    <div class="modal-body py-10 px-lg-17">

                                        <!-- Password field -->
                                        <div class="fv-row mb-8" ng-class="{'has-error': passwordForm.newPassword.$invalid && passwordForm.newPassword.$touched}">
                                            <label class="required fs-6 fw-semibold mb-2">Password</label>
                                            <div class="input-group">
                                                <input type="{{showNewPassword ? 'text' : 'password'}}"
                                                       placeholder="Password"
                                                       ng-model="NewPassword"
                                                       name="newPassword"
                                                       required
                                                       ng-minlength="8"
                                                       class="form-control bg-transparent"
                                                       ng-pattern="/^(?=.*[A-Z])(?=.*[0-9])(?=.*[\W_]).{8,}$/" />
                                                <span class="input-group-text" ng-click="togglePasswordVisibility('newPassword')" style="cursor: pointer;">
                                                    <i class="fa" ng-class="showNewPassword ? 'fa-eye-slash' : 'fa-eye'"></i>
                                                </span>
                                            </div>
                                            <div class="help-block" style="color:red;" ng-show="passwordForm.newPassword.$error.required && passwordForm.newPassword.$touched">
                                                Password is required.
                                            </div>
                                            <div class="help-block" style="color:red;" ng-show="passwordForm.newPassword.$error.minlength">
                                                Password must be exactly 8 characters long.
                                            </div>
                                            <div class="help-block" style="color:red;" ng-show="passwordForm.newPassword.$error.pattern">
                                                Password must contain at least one uppercase letter, one special character, and one number.
                                            </div>
                                        </div>


                                        <!-- Confirm Password field -->
                                        <div class="fv-row mb-8" ng-class="{'has-error': passwordForm.confirmPassword.$invalid && passwordForm.confirmPassword.$touched}">
                                            <label class="required fs-6 fw-semibold mb-2">Confirm Password</label>
                                            <div class="input-group">
                                                <input type="{{showConfirmPassword ? 'text' : 'password'}}"
                                                       placeholder="Repeat Password"
                                                       ng-model="ConfirmPassword"
                                                       name="confirmPassword"
                                                       required
                                                       ng-minlength="8"
                                                       class="form-control bg-transparent"
                                                       password-match="NewPassword" />
                                                <span class="input-group-text" ng-click="togglePasswordVisibility('confirmPassword')" style="cursor: pointer;">
                                                    <i class="fa" ng-class="showConfirmPassword ? 'fa-eye-slash' : 'fa-eye'"></i>
                                                </span>
                                            </div>
                                            <div class="help-block" style="color:red;" ng-show="passwordForm.confirmPassword.$error.passwordMatch && passwordForm.confirmPassword.$touched">
                                                Passwords do not match.
                                            </div>
                                        </div>


                                    </div>
                                    <!--end::Modal body-->
                                    <!--begin::Modal footer-->
                                    <div class="modal-footer flex-center">
                                        <!--begin::Button-->
                                        <button type="reset" data-bs-dismiss="modal" class="btn btn-light me-3">Discard</button>
                                        <!--end::Button-->
                                        <!--begin::Button-->
                                        <button type="submit" ng-disabled="passwordForm.$invalid" id="kt_modal_add_customer_submit" ng-click="SetPassword(passwordForm.$valid)" class="btn btn-primary">
