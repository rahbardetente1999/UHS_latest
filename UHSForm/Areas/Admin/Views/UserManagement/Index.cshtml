                                            <label class="fs-6 fw-semibold mb-2">
                                                <span class="required">Role</span>

                                            </label>
                                            <!--end::Label-->
                                            <!--begin::Input-->
                                            <select name="Role" id="ddRole" ng-model="txtRole" ng-change="GetTeambyRole()" aria-label="Select a Role" data-control="select2" data-placeholder="Select a Role..." data-dropdown-parent="#kt_modal_add_customer" class="form-select" required>
