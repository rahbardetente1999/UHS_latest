    <script>
        document.getElementById('dynamic-year').innerHTML = `${new Date().getFullYear()}&copy;`;
        // Get the current URL path
        var currentPath = window.location.pathname;

        // Find the menu items and accordion triggers
        var menuLinks = document.querySelectorAll('.menu-link');
        var accordionTriggers = document.querySelectorAll('[data-kt-menu-trigger="click"]');

        // Loop through each menu item
        menuLinks.forEach(function (menuItem) {
            // Get the href attribute of the menu item
            var href = menuItem.getAttribute('href');

            // Check if the current path matches the href attribute
            if (currentPath === href) {
                // Add the 'active' class to the menu item
                menuItem.classList.add('active');

                // If it's a submenu item, add 'active' class to its parent menu as well
                var parentMenu = menuItem.closest('.menu-item');
                if (parentMenu) {
                    parentMenu.classList.add('active');

                    // Find the closest accordion trigger and simulate a click event
                    var accordionTrigger = parentMenu.closest('[data-kt-menu-trigger="click"]');
                    if (accordionTrigger) {
                        // Add Bootstrap classes to open the accordion
                        accordionTrigger.classList.add('here');
                        accordionTrigger.classList.add('show');

                        // If using Bootstrap 5, you may need to use the Bootstrap JavaScript API to open the accordion:
                        // var accordion = new bootstrap.Collapse(accordionTrigger);
                        // accordion.show();
                    }
                }
            }
        });
    </script>

    <script>
        $(document).ready(function () {
            // Check if the tour has already been shown
            //if (!localStorage.getItem('tourShown')) {
            //    startTour();
            //}

            // Function to start the Intro.js tour
            function startTour() {
                var intro = introJs();

                intro.setOptions({
                    steps: [
                        {
                            element: '#new-booking-section',
                            intro: "Update Account Details",
                            position: 'bottom'
                        },
                        {
                            element: '.pendinglistL',
                            intro: "Check current bookings.",
                            position: 'top'
                        },
                        {
                            element: '.renewservice',
                            intro: "Renew Services",
                            position: 'right'
                        },
                        {
                            element: '.new_booking',
                            intro: "Add new booking",
                            position: 'left'
                        },
                        {
                            element: '.rescheduleserv',
                            intro: "Reschedule Services",
                            position: 'bottom'
                        }
                    ],
                    showProgress: true,
                    showBullets: true,
                    exitOnOverlayClick: false,
                    nextLabel: 'Next',
                    prevLabel: 'Back',
                    doneLabel: 'Finish'
                });

                intro.start();

                // Mark the tour as shown when it completes or is exited
                intro.oncomplete(function () {
                    localStorage.setItem('tourShown', 'true');
                });

                intro.onexit(function () {
                    localStorage.setItem('tourShown', 'true');
                });
            }

            //// Optional: Allow users to manually restart the tour via a button click
            $('#start-tour').on('click', function () {
                
                if (!localStorage.getItem('tourShown')) {
                    startTour();
                }
            });
        });


    </script>


